{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "eol-last" : 0,
        "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }],
        /*eslint linebreak-style: ["error", "windows"]*/
    }
}
